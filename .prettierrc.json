{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "vue/max-attributes-per-line": [
    "error",
    {
      "singleness": 1,
      "multiline": {
        "max": 1,
        "allowFirstLine": true
      }
    }
  ]
}
